{
    "compilerOptions": {
        "allowUnreachableCode": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "jsx": "react-native",
        "lib": [
            "es2019"
        ],
        "newLine": "lf",
        "noUnusedLocals": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "pretty": false,
        "removeComments": false,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "stripInternal": true,
        "target": "es2019"
    }
}